# EditorConfig is awesome: https://EditorConfig.org
# 适用于本项目的通用代码风格配置

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.yml]
indent_size = 2

[*.json]
indent_size = 2

[*.ts]
indent_size = 2

[*.js]
indent_size = 2

[*.vue]
indent_size = 2 