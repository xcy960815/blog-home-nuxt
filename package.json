{
  "name": "blog-home-nuxt",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "changelog": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s --commit-path . --date-format YYYY-MM-DD --output-unreleased",
    "changelog:first": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s -r 0 --date-format YYYY-MM-DD --output-unreleased",
    "changelog:version": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s -r 0 --release-count 0 --date-format YYYY-MM-DD --output-unreleased",
    "version": "conventional-changelog -c changelog.config.js -i CHANGELOG.md -s --date-format YYYY-MM-DD --output-unreleased && git add CHANGELOG.md && git commit -m \"docs: update changelog\"",
    "version:patch": "npm version patch -m \"chore(release): %s\" && npm run version",
    "version:minor": "npm version minor -m \"chore(release): %s\" && npm run version",
    "version:major": "npm version major -m \"chore(release): %s\" && npm run version"
  },
  "overrides": {
    "vue": "latest"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "4.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@nuxt/devtools": "latest",
    "@types/node": "^18.17.3",
    "@types/node-cron": "^3.0.1",
    "commitlint": "^17.6.7",
    "conventional-changelog-cli": "^5.0.0",
    "globals": "^16.2.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^15.5.2",
    "nuxt": "^3.9.1",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "pretty-quick": "^4.0.0",
    "sass": "^1.70.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "@antv/data-set": "^0.11.8",
    "@antv/g2": "^5.1.15",
    "@iconify/vue": "^4.1.1",
    "@pinia/nuxt": "^0.4.11",
    "chalk": "^5.2.0",
    "common-watermark": "^0.0.1",
    "dayjs": "^1.11.8",
    "element-plus": "^2.2.26",
    "monaco-editor": "^0.40.0",
    "mysql2": "^3.4.0",
    "node-cron": "^3.0.2",
    "pinia": "^2.1.4",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "vue-server-renderer": "^2.7.14",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "volta": {
    "node": "18.15.0"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "pretty-quick --staged"
    ]
  }
}
